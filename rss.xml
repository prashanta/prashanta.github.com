---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">

  <channel>
    <title>prashanta.me // Prashanta</title>
    <link>http://prashanta.me/</link>
    <atom:link href="http://prashanta.me/rss.xml" rel="self" type="application/rss+xml" />
    <description>Experiments, magic blue smoke, bugs</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %Z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %Z" }}</lastBuildDate>

    {% for post in site.posts %}
      <item>
        <title>{{ post.title }}</title>
        <link>http://www.spparker.com{{ post.url }}</link>
        <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %Z" }}</pubDate>
        <author>Prashanta</author>
        <guid>{{ site.path }}{{ post.url }}</guid>
        <description>{{ post.content | xml_escape }}</description>
      </item>
    {% endfor %}

  </channel>
</rss>
